        <template><![CDATA[<div class="modal-header">
  <span class="modal-title" id="modal-title">${Add unassigned tasks to Time Sheet}</span>
  <button type="button" class="btn icon-cross close pull-right" aria-label="${Close}" ng-click="cancel()"></button>
</div>
<div class="modal-body" id="modal-body">
  <p class="modal-text">${Search a task that you have worked during the week and add to the Time Sheet.}</p>
  <div class="row form-group">
    <span class="col-sm-12">
      <input id="task-selector" class="control select2" name="task-selector" type="hidden" aria-label="Task">
      <p ng-if="data.addTaskModal.isTaskIdEmpty" class="text-danger" style="margin:0;">${Task cannot be empty}</p>
    </span>
  </div>
</div>
<div class="modal-footer">
  <button class="btn btn-default" type="button" ng-click="cancel()">${Cancel}</button>
  <button class="btn btn-primary" type="button" ng-click="ok()">${OK}</button>
</div>
 <div ng-if="data.addTaskModal.loader" class="loader">
   <div class="loader-content">
     <fa name="spinner" spin="true"></fa>
   </div>
</div>]]></template>
    </sp_ng_template>
</record_update>
